{
    "name": "星云",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Tag Nebula",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "scripts": []
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "tag_nebula_ext",
        "default_popup": "index.html"
    },
    "omnibox": {
        "keyword": "tag_nebula_ext"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}